{
    "version": 2,
    "name": "feedmailu",
    "regions": ["lhr1", "dub1"],
    "build": {
        "env": {
            "API_URL": "@api_url",
            "REACT_APP_API_URL": "@api_url"
        }
    },
    "builds": [
        {
            "src": "/app/package.json",
            "use": "@now/next"
        },
        {
            "src": "/admin/package.json",
            "use": "@now/static-build",
            "config": { "distDir": "build" }
        }
    ],
    "routes": [
        { "src": "/favicon.ico", "dest": "/app/static/_meta/favicon.ico" },
        { "src": "/manifest.json", "dest": "/app/static/_meta/manifest.json" },
        { "src": "/browserconfig.xml", "dest": "/app/static/_meta/browserconfig.xml" },
        {
            "src": "/static/(?<folder>css|media|js)/(?<file>.*)",
            "headers": { "cache-control": "s-maxage=31536000,immutable" },
            "dest": "/admin/static/$folder/$file"
        },
        { "src": "/asset-manifest.json", "dest": "/admin/asset-manifest.json" },
        { "src": "/precache-manifest.(.*)", "dest": "/admin/precache-manifest.$1" },
        { "src": "/main.(.*).hot-update.js", "dest": "/admin/main.$1.hot-update.js" },
        { "src": "/sockjs-node/(.*)", "dest": "/admin/sockjs-node/$1" },
        {
            "src": "/service-worker.js",
            "headers": { "cache-control": "s-maxage=0" },
            "dest": "admin/service-worker.js"
        },
        {
            "src": "/admin",
            "status": 301,
            "headers": { "Location": "/admin/" }
        },
        {
            "src": "/admin/(.*)",
            "headers": { "cache-control": "s-maxage=0" },
            "dest": "/admin/index.html"
        },
        { "src": "/(.*)", "dest": "/app/$1" }
    ]
}
