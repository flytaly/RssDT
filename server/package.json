    "nodemailer-mock": "^1.5.8",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "prettier-eslint": "^13.0.0",
    "typescript": "^4.5.4"
  },
  "keywords": [],
