{
  "name": "rssdt-server",
  "version": "2.0.0",
  "description": "",
  "engines": {
    "node": ">=13"
  },
  "scripts": {
    "watch": "tsc -w",
    "build": "rimraf dist && tsc",
    "dev": "run-p start:dev mail-worker:dev",
    "start": "node dist/index.js",
    "start:dev": "nodemon dist/index.js -L | bunyan -o short",
    "lint": "eslint --ext .js,.ts ./src",
    "prettier:check": "prettier --check ./src",
    "prettier:write": "prettier --write ./src",
    "test": "ava **/*.test.js --serial",
    "test:watch": "ava ./dist/**/*.test.js -v --serial --watch",
    "test:codegen": "graphql-codegen --config codegen.yml",
    "getEnvType": "npx gen-env-types .env -o src/types/env.d.ts -e .",
    "feed-watcher": "node dist/feed-watcher/watcher.worker.js",
    "feed-watcher:dev": "nodemon dist/feed-watcher/watcher.worker.js | bunyan -o short",
    "mail-worker": "node dist/mail/mail.worker.js",
    "mail-worker:dev": "node dist/mail/mail.worker.js | bunyan -o short",
    "generate-migration": "drizzle-kit generate:pg --out drizzle --schema src/db/schema.ts",
    "migrate": "node dist/scripts/migrate.js",
    "migrate:test": "NODE_ENV=test npm run migrate"
  },
  "dependencies": {
    "@graphql-tools/schema": "^8.3.1",
    "apollo-server-express": "^3.6.1",
    "argon2": "^0.28.3",
