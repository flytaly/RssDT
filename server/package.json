    "@types/pg": "^8.10.2",
    "@types/sanitize-html": "^2.6.1",
    "@types/sinon": "^10.0.6",
    "@types/uuid": "^8.3.4",
    "@types/ws": "^8.2.2",
    "@types/yup": "^0.29.11",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "ava": "^4.0.1",
    "class-validator": "^0.13.1",
    "drizzle-kit": "^0.19.10",
    "eslint": "^8.6.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.1.0",
