    "bunyan": "^1.8.15",
    "cheerio": "^1.0.0-rc.5",
    "connect-redis": "^6.0.0",
    "cors": "^2.8.5",
    "dataloader": "^2.0.0",
    "dotenv-safe": "^8.2.0",
    "drizzle-orm": "^0.27.2",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "feedparser": "^2.2.10",
    "graphql-redis-subscriptions": "^2.3.1",
    "graphql-subscriptions": "^2.0.0",
    "graphql-ws": "^5.5.5",
    "handlebars": "^4.7.7",
    "iconv-lite": "^0.6.2",
    "image-size": "^1.0.1",
    "ioredis": "^4.28.3",
    "ioredis-mock": "^6.0.0",
    "joi": "^17.4.0",
    "jsdom": "^19.0.0",
    "lodash.pick": "^4.4.0",
    "luxon": "^2.3.0",
    "mjml": "^4.9.0",
    "nodemailer": "^6.5.0",
    "normalize-url": "^8.0.0",
    "p-queue": "^6.6.2",
    "pg": "^8.11.1",
    "pg-tsquery": "^8.2.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "sanitize-html": "^2.3.3",
    "sinon": "^12.0.1",
    "ts-node": "^10.4.0",
    "type-graphql": "^1.1.1",
    "uuid": "^8.3.2",
    "ws": "^8.4.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.9.0",
    "@graphql-codegen/typescript": "^2.4.2",
    "@graphql-codegen/typescript-graphql-request": "^4.3.3",
    "@graphql-codegen/typescript-operations": "^2.2.2",
    "@types/bunyan": "^1.8.6",
    "@types/connect-redis": "0.0.18",
    "@types/dotenv-safe": "^8.1.1",
    "@types/express": "^4.17.11",
