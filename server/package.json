  "scripts": {
    "start": "node src/index.js",
    "dev": "NODE_ENV=development nodemon -e js,graphql -x node src/index.js -i src/tests  | pino-pretty -c -t",
    "delete:dev": "prisma delete --env-file .env.dev --force",
    "deploy:prod": "prisma deploy",
    "deploy:dev": "prisma deploy --env-file .env.dev",
    "redis-run": "docker run -d --name redis-server --restart always -p 6379:6379 redis",
    "prisma-up": "docker-compose --file prisma/docker-compose.yml up -d",
    "prisma-down": "docker-compose --file prisma/docker-compose.yml down",
    "test": "NODE_ENV=test dotenv -e .env.dev -- jest --coverage",
    "test:watch": "NODE_ENV=test dotenv -e .env.dev -- jest --watch"
