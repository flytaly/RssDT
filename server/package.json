{
  "name": "rssdt-server",
  "version": "2.0.0",
  "description": "",
  "type": "module",
  "scripts": {
    "watch": "tsc -w",
    "build": "rimraf dist && tsc",
    "dev": "run-p start:dev mail-worker:dev",
    "start": "node dist/index.js",
    "start:dev": "nodemon dist/index.js -L | bunyan -o short",
    "start:docker-dev": "docker-compose -f docker-compose-dev.yml up",
    "lint": "eslint ./src",
    "prettier:check": "prettier --check ./src",
    "prettier:write": "prettier --write ./src",
    "test": "ava **/*.test.js --serial",
    "test:watch": "ava ./dist/**/*.test.js -v --serial --watch",
    "test:codegen": "graphql-codegen-esm  --config codegen.ts",
    "getEnvType": "npx gen-env-types .env -o src/types/env.d.ts -e .",
    "feed-watcher": "node dist/feed-watcher/watcher.worker.js",
    "feed-watcher:dev": "nodemon dist/feed-watcher/watcher.worker.js | bunyan -o short",
    "mail-worker": "node dist/mail/mail.worker.js",
    "mail-worker:dev": "node dist/mail/mail.worker.js | bunyan -o short",
    "generate-migration": "drizzle-kit generate --out drizzle --schema dist/db/schema.js --dialect postgresql",
    "migrate": "node dist/scripts/migrate.js",
    "migrate:test": "NODE_ENV=test npm run migrate"
  },
  "dependencies": {
    "@apollo/server": "^4.11.0",
    "@graphql-yoga/redis-event-target": "^3.0.1",
    "@graphql-yoga/subscription": "^5.0.1",
    "argon2": "^0.40.3",
    "axios": "^1.7.3",
    "bullmq": "^5.12.4",
    "bunyan": "^1.8.15",
    "cheerio": "^1.0.0",
    "connect-redis": "^7.1.1",
    "cors": "^2.8.5",
    "dataloader": "^2.2.2",
    "dotenv-safe": "^9.1.0",
    "drizzle-orm": "^0.33.0",
    "express": "^4.19.2",
    "express-session": "^1.18.0",
    "feedparser": "^2.2.10",
    "graphql": "^16.9.0",
    "graphql-subscriptions": "^2.0.0",
    "graphql-ws": "^5.16.0",
    "handlebars": "^4.7.8",
    "iconv-lite": "^0.6.3",
    "image-size": "^1.1.1",
    "ioredis": "^5.4.1",
    "ioredis-mock": "^8.9.0",
    "joi": "^17.13.3",
    "jsdom": "^24.1.1",
    "lodash.pick": "^4.4.0",
    "luxon": "^3.5.0",
    "mjml": "^4.15.3",
    "nodemailer": "^6.9.14",
    "normalize-url": "^8.0.1",
    "p-queue": "^8.0.1",
    "pg": "^8.12.0",
    "pg-tsquery": "^8.4.2",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.0.1",
    "sanitize-html": "^2.13.0",
    "sinon": "^18.0.0",
    "ts-node": "^10.9.2",
    "type-graphql": "^2.0.0-rc.2",
    "uuid": "^10.0.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@faker-js/faker": "^8.4.1",
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/typescript": "^4.0.9",
    "@graphql-codegen/typescript-graphql-request": "^6.2.0",
    "@graphql-codegen/typescript-operations": "^4.2.3",
    "@types/bunyan": "^1.8.11",
    "@types/dotenv-safe": "^8.1.6",
    "@types/eslint__js": "^8.42.3",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/feedparser": "^2.2.8",
    "@types/graphql": "^14.5.0",
    "@types/ioredis": "^4.28.10",
    "@types/jsdom": "^21.1.7",
    "@types/lodash.pick": "^4.4.9",
    "@types/luxon": "^3.4.2",
    "@types/maildev": "^0.0.7",
    "@types/mjml": "^4.7.4",
    "@types/node": "^22.2.0",
    "@types/nodemailer": "^6.4.15",
    "@types/pg": "^8.11.6",
    "@types/sanitize-html": "^2.11.0",
    "@types/sinon": "^17.0.3",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.5.12",
    "@types/yup": "^0.29.14",
    "ava": "^6.1.3",
    "drizzle-kit": "^0.24.0",
    "eslint": "^9.9.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
