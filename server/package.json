    "nodemailer-mock": "^2.0.6",
    "nodemon": "^3.1.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.1.0"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "imports": {
    "#entities": "./dist/entities/index.js",
    "#root/*": "./dist/*"
